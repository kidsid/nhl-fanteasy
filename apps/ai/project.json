{
  "name": "ai",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/ai/ranker/src",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/ai",
        "command": "ruff check ranker/src"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/ai",
        "command": "black ranker/src"
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/ai",
        "command": "python -m ranker.cli"
      }
    }
  }
}
